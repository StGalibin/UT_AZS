		ОбменСБанкамиСлужебный.ЗаполнитьСвойствоXDTO(Request, "receiver", "SBBOL_DBO", Истина, ТекстОшибки);
		
		ЗапросСертификатов = ОбменСБанкамиСлужебный.ОбъектТипаCML(ФабрикаXDTO,
			"Request.CryptoIncoming.Certificates", "http://bssys.com/upg/request");
			
		ОбменСБанкамиСлужебный.ЗаполнитьСвойствоXDTO(ЗапросСертификатов, "client", "1", , ТекстОшибки);
		ОбменСБанкамиСлужебный.ЗаполнитьСвойствоXDTO(ЗапросСертификатов, "bank", "1", , ТекстОшибки);
		
		ЗапросКриптографии = ОбменСБанкамиСлужебный.ОбъектТипаCML(
			ФабрикаXDTO, "Request.CryptoIncoming", "http://bssys.com/upg/request");

		ОбменСБанкамиСлужебный.ЗаполнитьСвойствоXDTO(ЗапросКриптографии, "Certificates", ЗапросСертификатов, , ТекстОшибки);
		ОбменСБанкамиСлужебный.ЗаполнитьСвойствоXDTO(Request, "CryptoIncoming", ЗапросКриптографии, Истина, ТекстОшибки);
		
		Request.Проверить();
			
		Запись = Новый ЗаписьXML;
		Запись.УстановитьСтроку("UTF-8");
		Запись.ЗаписатьОбъявлениеXML();
		ФабрикаXDTO.ЗаписатьXML(Запись, Request);
		ТекстЗапроса = Запись.Закрыть();
		
		Если ЗначениеЗаполнено(ТекстОшибки) Тогда
			ОбщегоНазначенияКлиентСервер.СообщитьПользователю(ТекстОшибки);
			Возврат Неопределено;
		КонецЕсли;

		
	Исключение
		ТекстСообщения = КраткоеПредставлениеОшибки(ИнформацияОбОшибке());
		Операция = НСтр("ru = 'Формирование запроса персональных данных организации'");
		ТекстОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		ЭлектронноеВзаимодействиеСлужебныйВызовСервера.ОбработатьОшибку(
			Операция, ТекстОшибки, ТекстСообщения, "ОбменСБанками");
		Возврат Неопределено;
	КонецПопытки;
		
	Возврат ТекстЗапроса;
	
КонецФункции

&НаСервереБезКонтекста
Функция ИдентификаторОрганизацииСбербанк(Знач ПерсональныеДанные)
	
	ЧтениеXML = Новый ЧтениеXML;
	ЧтениеXML.УстановитьСтроку(ПерсональныеДанные);
	Попытка
		ЭД = ФабрикаXDTO.ПрочитатьXML(ЧтениеXML);
	Исключение
		ЧтениеXML.Закрыть();
		ТекстСообщения = НСтр("ru = 'Возникла ошибка при чтении полученных данных из банка'");
		ТекстОшибки = НСтр("ru = 'Возникла ошибка при чтении полученных данных из банка
								|Текст ошибки: %1'");
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		ТекстОшибки = СтрШаблон(ТекстОшибки, ПодробноеПредставлениеОшибки);
		ВидОперации = НСтр("ru = 'Чтение полученных данных из банка'");
		ЭлектронноеВзаимодействиеСлужебныйВызовСервера.ОбработатьОшибку(
			ВидОперации, ТекстОшибки, ТекстСообщения, "ОбменСБанками");
		Возврат Неопределено;
	КонецПопытки;
	
	Если ЭД.Свойства().Получить("Errors") <> Неопределено Тогда
		ТекстСообщения = "";
		ОбменСБанкамиСлужебныйВызовСервера.ОбработатьИзвещениеСОшибкойСбербанк(ПерсональныеДанные, ТекстСообщения);
		ОбщегоНазначенияКлиентСервер.СообщитьПользователю(ТекстСообщения);
		Возврат Неопределено;
	КонецЕсли;
		
	Возврат ЭД.OrganizationsInfo.OrganizationInfo.OrgData.OrgId;
	
КонецФункции

&НаСервереБезКонтекста
Функция ЗаданиеПоПолучениюURLИнфоФайлаСбербанк(Знач URLВК)

	ПараметрыВыполнения = ДлительныеОперации.ПараметрыВыполненияВФоне(Новый УникальныйИдентификатор);
	ПараметрыВыполнения.НаименованиеФоновогоЗадания = НСтр("ru = 'Скачивание внешней компоненты с интернет.'");
	
	ПараметрыПроцедуры = Новый Структура("URLВК", URLВК);
	
	Возврат ДлительныеОперации.ВыполнитьВФоне(
		"ОбменСБанкамиСлужебный.ОпределитьURLИнфоФайла", ПараметрыПроцедуры, ПараметрыВыполнения);
	
КонецФункции

&НаСервереБезКонтекста
Процедура ЗагрузитьСертификатыСбербанка(Знач Данные, Знач Организация, Знач УникальныйИдентификатор, Успех, СертификатыКлиента, СертификатБанкаСсылка)
	
	Успех = Ложь;
	
	ЧтениеXML = Новый ЧтениеXML;
	ЧтениеXML.УстановитьСтроку(Данные);
	
	Попытка
		ЭД = ФабрикаXDTO.ПрочитатьXML(ЧтениеXML);
	Исключение
		ЧтениеXML.Закрыть();
		ТекстСообщения = НСтр("ru = 'Возникла ошибка при чтении полученных данных из банка'");
		ТекстОшибки = НСтр("ru = 'Возникла ошибка при чтении полученных данных из банка
							|Текст ошибки: %1'");
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		ТекстОшибки = СтрШаблон(ТекстОшибки, ПодробноеПредставлениеОшибки);
		ВидОперации = НСтр("ru = 'Чтение полученных данных из банка'");
		ЭлектронноеВзаимодействиеСлужебныйВызовСервера.ОбработатьОшибку(
			ВидОперации, ТекстОшибки, ТекстСообщения, "ОбменСБанками");
		Возврат;
	КонецПопытки;
	
	СертификатыКлиента = Новый Массив;
	
	Для Каждого ДанныеСертификата Из ЭД.Certificates.Certificate Цикл
		
		Bank = ЭлектронноеВзаимодействиеСлужебный.ЗначениеСвойстваXDTO(ДанныеСертификата, "Bank");
		active = ЭлектронноеВзаимодействиеСлужебный.ЗначениеСвойстваXDTO(ДанныеСертификата, "active");
		client = ЭлектронноеВзаимодействиеСлужебный.ЗначениеСвойстваXDTO(ДанныеСертификата, "client");
		
		Если Bank = "1" И active = "1" Тогда
			СертификатСтрокой = ДанныеСертификата.Последовательность().ПолучитьТекст(0);
			ДвоичныеДанныеСертификата = Base64Значение(СертификатСтрокой);
			ДвоичныеДанныеСертификата = ОбменСБанкамиСлужебный.ДанныеСертификатаВФорматеDER(ДвоичныеДанныеСертификата);
			СертификатБанкаСсылка = ПоместитьВоВременноеХранилище(ДвоичныеДанныеСертификата, УникальныйИдентификатор);
		ИначеЕсли client = "1" И active = "1" Тогда
			СертификатСтрокой = ДанныеСертификата.Последовательность().ПолучитьТекст(0);
			ДвоичныеДанныеСертификата = Base64Значение(СертификатСтрокой);
			ДвоичныеДанныеСертификата = ОбменСБанкамиСлужебный.ДанныеСертификатаВФорматеDER(ДвоичныеДанныеСертификата);
			НовыйСертификат = ОбменСБанкамиСлужебныйВызовСервера.СоздатьСертификатСбербанка(
				ДвоичныеДанныеСертификата, Организация);
			Если НЕ ЗначениеЗаполнено(НовыйСертификат) Тогда
				Возврат;
			КонецЕсли;
			
			СертификатыКлиента.Добавить(НовыйСертификат);
			
		КонецЕсли;
		
	КонецЦикла;
	
	Успех = Истина;
	
КонецПроцедуры

